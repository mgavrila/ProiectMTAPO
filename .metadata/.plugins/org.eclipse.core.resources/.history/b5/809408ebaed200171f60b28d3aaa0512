package GUI;

import java.awt.Color;

import javax.swing.JFrame;

public class GUI {

	JFrame window = new JFrame("GUI");
	Login login = new Login();
	
	
	
	public GUI(){
		window.add(login);
		
		window.setSize(400,400);
		window.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		window.pack();
		window.setVisible(true);
	}
}
